class ArrayStack :
  def __init__(self) :
    self.data = [] # 빈 스택으로 초기화
  pass # constructor 
  
  def size(self) :
    return len(self.data) # 스택의 크기를 반환하는 메소드
  pass # size
  
  def isEmpty(self) :
    return self.size() == 0 # 스택이 비어있는지 판단하여 True 또는 False 반환
  pass # isEmpty
  
  def push(self, item) :
    self.data.append(item) # 데이터 원소를 추가
  pass # push
  
  def pop(self):
    return self.data.pop() # 데이터 원소를 반환(해당 원소는 삭제됨)
  pass # pop
  
  def peek(self) :
    return self.data[-1] # 스택의 꼭대기 원소를 반환(해당 원소는 삭제x)
  pass # peek
  
 pass # ArrayStack
   
